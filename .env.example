# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# App Configuration
VITE_APP_NAME=Otagon
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# Developer Mode
VITE_DEV_MODE_PASSWORD=dev123

# OAuth Configuration (Optional - for production)
VITE_GOOGLE_CLIENT_ID=your_google_client_id
VITE_DISCORD_CLIENT_ID=your_discord_client_id

# API Configuration
# âœ… PRODUCTION: Gemini API key is stored securely in Supabase Edge Function secrets (GEMINI_KEY)
# The client-side app does NOT need any AI API keys - all calls go through secure server-side proxy
# 
# For local development/testing only (if USE_EDGE_FUNCTION=false in aiService.ts):
# VITE_GEMINI_API_KEY=your_gemini_api_key
#
# Note: For public release, migrate to Vertex AI - see VERTEX_AI_SETUP.md

# Feature Flags
VITE_USE_ROUTER=true

# Analytics & Monetization (Optional)
# Google Analytics 4 Measurement ID (format: G-XXXXXXXXXX)
VITE_GA_MEASUREMENT_ID=

# Microsoft Clarity Project ID (format: XXXXXXXXXX)
VITE_CLARITY_PROJECT_ID=

# Google AdSense Publisher ID (format: ca-pub-XXXXXXXXXXXXXXXX)
# Only needed when implementing AdSense for free tier ads
VITE_ADSENSE_PUBLISHER_ID=
VITE_ADSENSE_ENABLED=false
VITE_ADSENSE_TEST_MODE=true
